{
  "id": "cc9344fb",
  "name": "Mixbox Palette",
  "version": "1.0.1",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0"
    }
  ],
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "mixboxpalette",
      "label": "Mixbox Palette",
      "minimumSize": { "width": 400, "height": 600 },
      "maximumSize": { "width": 2000, "height": 2000 },
      "preferredDockedSize": { "width": 400, "height": 600 },
      "preferredFloatingSize": { "width": 600, "height": 600 },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/pluginIcon.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "light", "lightest", "all"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/pluginIcon.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "light", "lightest", "all"]
    }
  ],
  "requiredPermissions": {
    "webview": {
      "allow": "yes",
      "domains": [
        "https://food211.github.io",
        "https://mixbox-palette.pages.dev"
      ],
      "enableMessageBridge": "localAndRemote"
    },
    "network": {
      "domains": [
        "https://food211.github.io",
        "https://mixbox-palette.pages.dev"
      ]
    }
  }
}